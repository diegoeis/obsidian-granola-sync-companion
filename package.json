    "@types/node": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.30.0",
    "@typescript-eslint/parser": "^5.30.0",
    "esbuild": "^0.18.0",
    "fs-extra": "^11.1.0",
    "nodemon": "^3.0.1",
    "obsidian": "^1.4.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "fs-extra": "^11.1.0"
  }
}