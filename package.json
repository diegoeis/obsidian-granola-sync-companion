  "description": "Companion plugin for Obsidian Granola Sync to prevent note duplication and extend functionality.",
  "main": "main.js",
  "scripts": {
    "dev": "node build.js",
    "build": "NODE_ENV=production node build.js",
    "install:dev": "npm run build && cp -r dist/* /Users/diegoeis/obs-notes/.obsidian/plugins/granola-plugin-companion",
    "watch": "nodemon --watch 'src/**/*.ts' --exec 'npm run dev'"
  },
  "keywords": [
